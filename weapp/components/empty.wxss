.empty {
  text-align: center;
  padding: 60rpx 30rpx;
  color: #b2b2b2;
}
.empty .ic-empty {
  width: 200rpx;
  height: 200rpx;
}
.empty .des {
  font-size: 28rpx;
  margin-top: 20rpx;
}
.empty .content {
  margin-top: 30rpx;
}
.empty .btn {
  height: 72rpx;
  line-height: 72rpx;
  border-radius: 72rpx;
  padding: 0rpx 64rpx;
  display: inline-block;
  background: #fae37c;
  color: #13100e;
  border: none;
  box-shadow: 4rpx 8rpx 12rpx rgba(250, 227, 124, 0.3);
  font-size: 34rpx;
}
.empty .btn::after {
  border: none;
}
.empty .btn.btn-hover {
  background: rgba(250, 227, 124, 0.5);
}

